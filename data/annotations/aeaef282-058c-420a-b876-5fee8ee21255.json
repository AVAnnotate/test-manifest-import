      "uuid": "7ba29f04-6e85-4b0b-8020-89f9e4077b1e"
    },
    {
      "start_time": 1231.53415,
      "end_time": 1232.805442,
      "annotation": [
        {
          "text": "Alright. Let's play."
        }
      ],
      "tags": [
        {
